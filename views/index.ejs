	<script>
		
		var slideIndex = 1;
		showSlides(slideIndex);

		function plusSlides(n) {
			showSlides(slideIndex += n);
		}

		function currentSlide(n) {
			showSlides(slideIndex = n);
		}

		function showSlides(n) {
			var i;
			var slides = document.getElementsByClassName("mySlides");

			if (n > slides.length) { slideIndex = 1 }
			if (n < 1) { slideIndex = slides.length }
			for (i = 0; i < slides.length; i++) {
				slides[i].style.display = "none";
			}

			slides[slideIndex - 1].style.display = "block";

		}
		var clicked = false;
		function displayOptions(){
			if(clicked == false){
				var logOut = document.createElement('div');
				var invoice = document.createElement('div');
				var forgot_pswd = document.createElement('div');
				var iconMoF = document.getElementById('iconMoF');
				iconMoF.style.overflow = 'auto';
				invoice.setAttribute('id','invoice');
				invoice.setAttribute('onclick','displayInvoice()');
				invoice.innerHTML = 'Invoice..';
				iconMoF.appendChild(invoice);
				forgot_pswd.setAttribute('id','logoutBtn');
				forgot_pswd.setAttribute('onclick','changePswd()');
				forgot_pswd.innerHTML = 'Change Password..';
				iconMoF.appendChild(forgot_pswd);
				logOut.setAttribute('id','logoutBtn');
				logOut.setAttribute('onclick','logout()');
				logOut.innerHTML = 'Log Out..';
				iconMoF.appendChild(logOut);
				clicked = true;
				return;
			}
			if(clicked == true){
				var logOut = document.getElementById('logoutBtn');
				logOut.style.display = 'none';
				logOut.style.visibility = 'hidden';
				var invoice = document.getElementById('invoice');
				invoice.style.display = 'none';
				invoice.style.visibility = 'hidden';
				clicked = false;
				return;
			}
		}
		function changePswd(){
			window.location = 'http://localhost:5000/gym-snippets/forgot_pswd.html?useremail='+localStorage.email;
		}
		function undisplayOptions(){
			var logOut = document.getElementById('logoutBtn');
			logOut.style.display = 'none';
			logOut.style.visibility = 'hidden';
			var invoice = document.getElementById('invoice');
			invoice.style.display = 'none';
			invoice.style.visibility = 'hidden';
		}
		function logout(){
			//alert('inside');
			localStorage.username = undefined;
			localStorage.email = undefined;
			localStorage.mode = undefined;
			localStorage.loggedIN = undefined;
			window.location = 'http://localhost:5000/gym-snippets/loginform.html';
		}
		function displayInvoice(){
			//alert(window.location);
			var url = window.location.toString();
			url = url.replace('index','invoice');
			//alert(url);
			window.location = url;
		}
	</script>
